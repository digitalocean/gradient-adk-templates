# DigitalOcean API Token (for deployment and database setup)
DIGITALOCEAN_API_TOKEN=your_digitalocean_api_token

# Gradient Model Access Key (for LLM inference)
GRADIENT_MODEL_ACCESS_KEY=your_gradient_model_access_key

# Database Configuration (populated by setup script)
DB_TYPE=postgres
DB_HOST=your-db-host.db.ondigitalocean.com
DB_PORT=25060
DB_NAME=flights_db
DB_USER=readonly_user
DB_PASSWORD=your_readonly_password
DB_SSL_MODE=require

# For setup script only (admin credentials)
DB_ADMIN_USER=doadmin
DB_ADMIN_PASSWORD=your_admin_password
